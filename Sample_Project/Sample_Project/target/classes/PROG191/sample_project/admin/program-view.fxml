<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<Pane prefHeight="500.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PROG191.sample_project.controllers.admin.ProgramController">
    <children>
        <VBox alignment="TOP_CENTER" prefHeight="500.0" prefWidth="1000.0">
            <children>
                <HBox alignment="CENTER_LEFT" prefHeight="50.0" spacing="25.0">
                    <children>
                        <Label alignment="CENTER" prefHeight="25.0" prefWidth="50.0" text="Name" textAlignment="CENTER" textFill="#00704a" />
                        <TextField fx:id="searchName" prefHeight="25.0" prefWidth="250.0" style="-fx-border-color: #00704A;" />
                        <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="100.0" style="-fx-background-color: #00704A;" text="Search" textFill="WHITE" />
                    </children>
                    <padding>
                        <Insets left="25.0" />
                    </padding>
                </HBox>
                <HBox alignment="CENTER" prefHeight="415.0">
                    <children>
                        <TableView fx:id="listTable" editable="true" prefWidth="490.0" style="-fx-border-color: #00704A;" tableMenuButtonVisible="true">
                     <HBox.margin>
                        <Insets left="5.0" right="5.0" />
                     </HBox.margin></TableView>
                        <VBox alignment="TOP_CENTER" onKeyPressed="#addEnter" prefWidth="495.0" style="-fx-border-color: #00704A;">
                            <padding>
                                <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
                            </padding>
                            <children>
                                <HBox alignment="CENTER" prefHeight="50.0" spacing="25.0">
                                    <children>
                                        <Label alignment="CENTER" prefHeight="25.0" prefWidth="50.0" text="Name" textAlignment="CENTER" textFill="#00704a" />
                                        <TextField fx:id="addName" prefHeight="25.0" prefWidth="250.0" style="-fx-border-color: #00704A;" />
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER" prefHeight="50.0" spacing="25.0">
                                    <children>
                                        <Button fx:id="btnAdd" mnemonicParsing="false" onMouseClicked="#add" prefHeight="25.0" prefWidth="100.0" style="-fx-background-color: #00704A;" text="Add" textFill="WHITE" />
                                        <Button fx:id="btnEdit" disable="true" mnemonicParsing="false" onMouseClicked="#edit" prefHeight="25.0" prefWidth="100.0" style="-fx-background-color: #00704A;" text="Edit" textFill="WHITE" />
                                    </children>
                                </HBox>
                        <Label fx:id="lblId" textFill="TRANSPARENT" />
                            </children>
                     <HBox.margin>
                        <Insets right="5.0" />
                     </HBox.margin>
                        </VBox>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</Pane>
