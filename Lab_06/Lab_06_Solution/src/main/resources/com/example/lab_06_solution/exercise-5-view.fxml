<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Circle?>

<Pane onMouseMoved="#mouseMove" prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab_06_solution.E5Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <children>
        <Circle fx:id="circle" fill="WHITE" layoutX="150.0" layoutY="150.0" radius="50.0" stroke="BLACK"
                strokeType="INSIDE"/>
        <Label fx:id="label" layoutX="135.0" layoutY="14.0"/>
    </children>
</Pane>
