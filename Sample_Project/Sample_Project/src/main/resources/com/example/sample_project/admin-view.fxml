<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.Pane?>

<Pane fx:id="mainPane" prefHeight="500.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sample_project.AdminController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <TabPane prefHeight="500.0" prefWidth="1000.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab text="Program">
                    <content>
                        <fx:include source="admin-program-view.fxml" />
                    </content>
                </Tab>
                <Tab text="Course">
                    <content>
                        <fx:include source="admin-course-view.fxml" />
                    </content>
                </Tab>
                <Tab text="User">
                    <content>
                        <fx:include source="admin-user-view.fxml" />
                    </content>
                </Tab>
                <Tab text="Session">
                    <content>
                        <fx:include source="admin-session-view.fxml" />
                    </content>
                </Tab>
            </tabs>
        </TabPane>
        <Button layoutX="920.0" layoutY="2.0" mnemonicParsing="false" onMouseClicked="#logout" prefHeight="25.0" prefWidth="75.0" style="-fx-background-color: #00704A;" text="Logout" textFill="WHITE" />
    </children>
</Pane>
